{
    "manifest_version": 3,
    "name": "Gradient Generator",
    "version": "1.0",
    "description": "Gradient Generator extension built in pure HTML CSS JS...",
    "author" : "ajay-dhangar",
    "icons": {
        "48": "colours-48.png",
        "128": "colours-128.png"
    },
    "permissions": [
        "storage"
    ],
   
    "action": {
        "default_popup": "index.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "index.html"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}